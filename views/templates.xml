        <template id="exception" name="Reset Button">
            <head>
                <t t-call-assets="web.assets_common"/>
                <title>Drag and Drop PDF Invoice</title>
                <link rel="shortcut icon" sizes="196x196" href="/shopify_order_printer/static/description/Logo.png"/>
                <link rel="stylesheet" href="/web/static/lib/bootstrap/dist/css/bootstrap.css"/>
            </head>
            <body>
                <div class="card mx-auto mt-5" style="width: 768px">
                    <div class="card-header">
                        Error
                    </div>
                    <div class="card-body">
                        <span class="message">Error. Please refresh your browser! If the problem persists, please
                            contact
                            the administrator or hit the Reset button to start over.
                        </span>
                        <form id="disconnect" t-attf-action="#{reset_action}" method="POST">
                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/><br/>
                            <button class="btn btn-secondary" type="submit" value="Submit">Reset</button>
                        </form>
                    </div>
                </div>
            </body>
        </template>
        <template id="404_not_found" name="Not Found">
            <head>
                <t t-call-assets="web.assets_common"/>
                <title>Drag and Drop PDF Invoice</title>
                <link rel="shortcut icon" sizes="196x196" href="/shopify_order_printer/static/description/Logo.png"/>
            </head>
            <body>
                <div class="card mx-auto mt-5 border-0" style="max-width: 900px">
                    <div class="card-body">
                        <div class="alert alert-danger text-center" role="alert">
                            Page error, some element can not load or Session expired.Ensure your template is format-well
                            with PDF display.
                            Try load your template again or contact the administrator for
                            support !
                        </div>
                    </div>
                </div>
            </body>
        </template>

        <template id="preview" name="Shopify PDF Preview">
